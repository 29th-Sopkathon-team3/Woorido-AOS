<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.sopt.study.woorido">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Woorido">
        <activity
            android:name=".detail.ui.RankingActivity"
            android:exported="false" />
        <activity
            android:name=".detail.ui.RoomCreateActivity"
            android:exported="false"
            android:label="@string/title_activity_room_create"
            android:theme="@style/Theme.Woorido.NoActionBar" />
        <activity
            android:name=".detail.ui.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>